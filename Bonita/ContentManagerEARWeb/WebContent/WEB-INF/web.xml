<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" 
    id="WebApp_ID" version="3.0">
    <display-name>ContentManager</display-name>
    <description>ContentManager</description>

	<servlet>
		<servlet-name>ContentManagerServlet</servlet-name>
		<servlet-class>pe.bbvacontinental.medios.gestiondemanda.contentmanagerws.ws.SimpleContentManagerWS</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>ContentManagerServlet</servlet-name>
		<url-pattern>/ContentManagerServiceSB12</url-pattern>
	</servlet-mapping>

    <welcome-file-list>
        <welcome-file>redirect.jsp</welcome-file>
    </welcome-file-list>
</web-app>